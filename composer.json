{
    "name": "spatie/laravel-mobile-pass",
    "description": "Generate mobile passes for iOS and Android",
    "keywords": [
        "Spatie",
        "laravel",
        "laravel-mobile-pass"
    ],
    "homepage": "https://github.com/spatie/laravel-mobile-pass",
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Johnson",
            "email": "github@danjohnson.xyz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^12.0",
        "pkpass/pkpass": "^2.3.2",
        "spatie/laravel-package-tools": "^1.19"
    },
    "require-dev": {
        "laravel/pint": "^1.21.2",
        "nunomaduro/collision": "^8.7.0",
        "larastan/larastan": "^3.2",
        "orchestra/testbench": "^10.1.0",
        "pestphp/pest": "^3.7.4",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.1",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan-deprecation-rules": "^2.0.1",
        "phpstan/phpstan-phpunit": "^2.0.4",
        "spatie/laravel-ray": "^1.40.1"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\LaravelMobilePass\\": "src/",
            "Spatie\\LaravelMobilePass\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\LaravelMobilePass\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\LaravelMobilePass\\LaravelMobilePassServiceProvider"
            ],
            "aliases": {
                "LaravelMobilePass": "Spatie\\LaravelMobilePass\\Facades\\LaravelMobilePass"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
